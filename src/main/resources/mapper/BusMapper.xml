<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shuaib.mapper.BusMapper">

    <resultMap id="BaseResultMap" type="com.shuaib.bean.Bus">
            <id property="busId" column="bus_id" jdbcType="BIGINT"/>
            <result property="busName" column="bus_name" jdbcType="VARCHAR"/>
            <result property="cityCode" column="city_code" jdbcType="VARCHAR"/>
            <result property="type" column="type" jdbcType="TINYINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        bus_id,bus_name,city_code,
        type,create_time,update_time
    </sql>
</mapper>
